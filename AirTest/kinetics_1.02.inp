N2(v5) + N => N2(v6) + N                           !   kVT01_N2N * 6.0d0
N2(v6) + N => N2(v7) + N                           !   kVT01_N2N * 7.0d0
N2(v7) + N => N2(v8) + N                           !   kVT01_N2N * 8.0d0

N2(v1) + O  => N2     + O                          !   kVT10_N2O
