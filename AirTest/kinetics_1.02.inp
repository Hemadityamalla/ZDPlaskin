N2(v6) + N => N2(v5) + N                           !   kVT10_N2N * 6.0d0
N2(v7) + N => N2(v6) + N                           !   kVT10_N2N * 7.0d0
N2(v8) + N => N2(v7) + N                           !   kVT10_N2N * 8.0d0
N2     + N => N2(v1) + N                           !   kVT01_N2N
N2(v1) + N => N2(v2) + N                           !   kVT01_N2N * 2.0d0
N2(v2) + N => N2(v3) + N                           !   kVT01_N2N * 3.0d0
N2(v3) + N => N2(v4) + N                           !   kVT01_N2N * 4.0d0
