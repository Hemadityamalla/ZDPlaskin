e + O2 => e + O2(v4)                               !   BOLSIG O2 -> O2(v4)
e + O2(v1) => e + O2                               !   BOLSIG O2(v1) -> O2
e + O2(v2) => e + O2                               !   BOLSIG O2(v2) -> O2
e + O2(v3) => e + O2                               !   BOLSIG O2(v3) -> O2
e + O2(v4) => e + O2                               !   BOLSIG O2(v4) -> O2

