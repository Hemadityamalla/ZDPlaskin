ELEMENTS
e N O
END

#==================================================================================================================================

BOLSIG
N2 N2(v1) N2(v2) N2(v3) N2(v4) N2(v5) N2(v6) N2(v7) N2(v8) N2(A3)
DENSITY N2 += N2(v1)
DENSITY N2 += N2(v2)
DENSITY N2 += N2(v3)
DENSITY N2 += N2(v4)
DENSITY N2 += N2(v5)
DENSITY N2 += N2(v6)
DENSITY N2 += N2(v7)
DENSITY N2 += N2(v8)
O2 O2(v1) O2(v2) O2(v3) O2(v4) O2(a1)
DENSITY O2 += O2(v1)
DENSITY O2 += O2(v2)
DENSITY O2 += O2(v3)
DENSITY O2 += O2(v4)
N O O3 NO
END

#==================================================================================================================================

SPECIES

# NITROGEN: N2 = N2(X1), N = N(4S)
#           N2(W3,B`3)  -> N2(B3)
#           N2(a1,w1)   -> N2(a`1)
#           N2(E3,a``1) -> N2(C3)
#           N2(SUM) -> N + N(2D)
N2
N2(v1) N2(v2) N2(v3) N2(v4) N2(v5) N2(v6) N2(v7) N2(v8)
N2(A3) N2(B3) N2(a`1) N2(C3)
N N(2D) N(2P)
N^+ N2^+ N3^+ N4^+

# OXYGEN:   O2 = O2(X3), O = O(3P)
#           O2(4.5eV)  = O2(A3) + O2(C3) + O2(c1)
#           O2(6.0eV)  -> O + O
#           O2(8.4eV)  -> O + O(1D)
#           O2(9.97eV) -> O + O(1S)
O2
O2(v1) O2(v2) O2(v3) O2(v4)
O2(a1) O2(b1) O2(4.5eV)
O O(1D) O(1S)
O3
O^+ O2^+ O4^+
O^- O2^- O3^- O4^-

# NITROGEN OXIDE: NO = NO(X2)
NO
NO^+
NO^-

# MISC
O2^+N2
E

END

