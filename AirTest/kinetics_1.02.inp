N2(v5) + N2 => N2(v6) + N2                         !   kVT01_N2N2 * 6.0d0
N2(v6) + N2 => N2(v7) + N2                         !   kVT01_N2N2 * 7.0d0
N2(v7) + N2 => N2(v8) + N2                         !   kVT01_N2N2 * 8.0d0

N2(v1) + N => N2     + N                           !   kVT10_N2N
N2(v2) + N => N2(v1) + N                           !   kVT10_N2N * 2.0d0
